{
    "manifest_version": 2,
    "name": "Kami-sama",
    "version": "0.0.1",
    "description": "The ultimate pixiv extension",
    "icons": {
        "128": "res/pa-icon-128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.pixiv.net/*", "*://*.pximg.net/*"],
            "js": ["content.min.js"]
        }
    ]
}